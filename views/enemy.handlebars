<h1>Enemies</h1>
<div class="item">
	<img src="{{icon}}" title="{{name}}" alt="{{name}} icon"/>
	<h2>{{name}}</h2>
	<div>
		<img src="{{image}}" title="{{name}}" alt="{{name}} portrait"/>
		<h3 id="description">Description</h3>
		<p>{{description}}</p>
		<div>
			<h3 id="notes">Notes</h3>
			{{#each notes}}
				<h4>{{title}}</h4>
				<p>{{content}}</p>
			{{/each}}
		</div>
	</div>
</div>